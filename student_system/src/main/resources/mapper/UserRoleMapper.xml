<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lb.mapper.UserRoleMapper">
	<resultMap id="role_relationship" type="com.lb.entity.Role">
		<id column="id" property="id"></id>
		<result column="user_id" property="userId"></result>
		<result column="role_id" property="roleId"></result>
	</resultMap>
	<!--添加用户角色关系-->
	<insert id="insertUserRole" parameterType="com.lb.entity.Role" >
		insert into t_user_role(user_id,role_id) values (#{userId},#{roleId})
	</insert>
</mapper>
